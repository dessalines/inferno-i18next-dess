{
	"name": "inferno-i18next",
	"description": "A translation utility for Inferno components using the i18next ecosystem.",
	"version": "7.1.12",
	"keywords": [
		"i18n",
		"l10n",
		"internationalization",
		"localization",
		"translation",
		"inferno",
		"i18next"
	],
	"license": "MIT",
	"main": "dist/bundle.js",
	"browser": "dist/bundle.js",
	"jsnext:main": "src/index.js",
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"babel-jest": "^24.7.1",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"babel-plugin-inferno": "^6.0.5",
		"codecov": "^3.3.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-inferno": "^7.11.0",
		"i18next": "^15.1.0",
		"jest": "^24.7.1",
		"rollup": "^1.10.1",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-uglify": "^6.0.2"
	},
	"dependencies": {
		"html-parse-stringify2": "^2.0.1",
		"inferno": "^7.1.12",
		"inferno-clone-vnode": "^7.1.12",
		"inferno-create-element": "^7.1.12",
		"inferno-shared": "^7.1.12",
		"inferno-vnode-flags": "^7.1.12"
	},
	"scripts": {
		"build": "rollup -c",
		"build-all": "npm run build-dev && npm run build-prod",
		"build-dev": "cross-env NODE_ENV=development npm run build",
		"build-prod": "cross-env NODE_ENV=production npm run build",
		"test": "eslint src && jest",
		"upload-cov": "codecov"
	}
}
