{
	"name": "inferno-i18next",
	"description": "A translation utility for Inferno components using the i18next ecosystem.",
	"version": "6.0.3",
	"keywords": [
		"i18n",
		"l10n",
		"internationalization",
		"localization",
		"translation",
		"inferno",
		"i18next"
	],
	"license": "MIT",
	"main": "dist/bundle.js",
	"browser": "dist/bundle.js",
	"jsnext:main": "src/index.js",
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.6.0",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"babel-plugin-inferno": "^6.0.0",
		"codecov": "^3.1.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.7.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-inferno": "^7.11.0",
		"i18next": "^10.6.0",
		"jest": "^23.6.0",
		"rollup": "^0.66.6",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-uglify": "^6.0.0"
	},
	"dependencies": {
		"html-parse-stringify2": "^2.0.1",
		"inferno": "^6.0.3",
		"inferno-clone-vnode": "^6.0.3",
		"inferno-create-element": "^6.0.3",
		"inferno-shared": "^6.0.3",
		"inferno-vnode-flags": "^6.0.3"
	},
	"scripts": {
		"build": "rollup -c",
		"build-all": "npm run build-dev && npm run build-prod",
		"build-dev": "cross-env NODE_ENV=development npm run build",
		"build-prod": "cross-env NODE_ENV=production npm run build",
		"test": "eslint src && jest",
		"upload-cov": "codecov"
	}
}
