{
	"name": "inferno-i18next-dess",
	"description": "A translation utility for Inferno components using the i18next ecosystem.",
	"version": "7.4.2",
	"keywords": [
		"i18n",
		"l10n",
		"internationalization",
		"localization",
		"translation",
		"inferno",
		"i18next"
	],
	"license": "MIT",
	"main": "dist/bundle.js",
	"browser": "dist/bundle.js",
	"jsnext:main": "src/index.js",
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"babel-jest": "^26.1.0",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"babel-plugin-inferno": "^6.1.1",
		"codecov": "^3.7.2",
		"cross-env": "^7.0.2",
		"eslint": "^7.5.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-inferno": "^7.20.1",
		"i18next": "^19.6.2",
		"jest": "^26.1.0",
		"rollup": "^2.22.2",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-uglify": "^6.0.4"
	},
	"dependencies": {
		"html-parse-stringify2": "^2.0.1",
		"inferno": "^7.4.2",
		"inferno-clone-vnode": "^7.4.2",
		"inferno-create-element": "^7.4.2",
		"inferno-shared": "^7.4.2",
		"inferno-vnode-flags": "^7.4.2"
	},
	"scripts": {
		"build": "rollup -c",
		"build-all": "npm run build-dev && npm run build-prod",
		"build-dev": "cross-env NODE_ENV=development npm run build",
		"build-prod": "cross-env NODE_ENV=production npm run build",
		"test": "eslint src && jest",
		"upload-cov": "codecov"
	}
}
